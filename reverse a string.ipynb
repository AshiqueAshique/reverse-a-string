   "source": [
    "def reverse(s):                 #function to reverse a string\n",
    "    for i in range((len(s)-1),-1,-1):\n",
    "        print(s[i], end = \"\")\n",
    "\n",
    "s = \"1234abcd\"\n",
    "reverse(s)          #function calling"
   ]
